<div class="top-item">
  <h1>在室管理</h1>
  <div class="item_name">
  <table>
    <thead>
      <tr>
        <th>名前</th>
        <th>入室(予定)時間</th>
        <th>退出予定時間</th>
      </tr>
    </thead>
  </div>
  <% @member.each do |mem| %>
    <tbody>
      <tr>
        <th><%= link_to mem.name, "/#{mem.id}", class:"user_name" %></th>
        <th>時間</th>
        <% if mem.plan.min < 10%>
          <% min = "0" + mem.plan.min.to_s%>
        <% else %>
          <% min = mem.plan.min.to_s%>
        <% end %>
        <% time = mem.plan.hour.to_s + ":" + min%>
        <th><%= "#{time}"%></th>
      </tr>
    </tbody>
  <% end %>
  </table>

  <div>
    <%= link_to "入室", "/enter", class: "under_item" %>
    <%= link_to "予約", "/res", class: "under_item" %>
  </div>
  <%="本日の利用人数は#{@cnt.counter}人です"%>
</div>

      <%
=begin%>
 <% if Time.now&.strftime("%H:%M") >= mem.plan&.strftime("%H:%M") %>
        <%= link_to("/#{mem.id}/destroy", {method: "post"}) do %>
          <button id="click">a</button>
        <% end %>
        <script>document.getElementById("click").click()</script>
      <% end %>
<%
=end%>

