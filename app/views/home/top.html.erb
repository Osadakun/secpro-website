<h1>在室管理</h1>
<div>
	<% @member.each do |mem| %>
		<%= link_to(mem.name, "/#{mem.id}") %>
		<%= mem.plan%>
	<% end%>
</div>
<%= form_tag('/create') do %>
	<div>
		<label>名前</label>
	</div>
	<textarea name="name"></textarea>
	<div>
  	<label for="scheduled-time">帰宅予定時間</label>
  </div>
	<input type="time" name="times" id="scheduled-time">
	<div>
		<button type="submit">確定</button>
	</div>
<% end %>